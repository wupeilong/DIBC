<?xml version="1.0" encoding="UTF-8"?> 
	<!DOCTYPE mapper PUBLIC "-//ibatis.apache.org//DTD Mapper 3.0//EN" 
		"http://ibatis.apache.org/dtd/ibatis-3-mapper.dtd">
<mapper namespace="cn.dibcbks.mapper.UnqualifiedMapper">
	<resultMap id="BaseResultMap" type="cn.dibcbks.entity.Unqualified">
	    <id column="id" property="id" />
	    <result column="unit_id" property="unitId" />
	    <result column="unit_name" property="unitName" />
	    <result column="type" property="type" />
	    <result column="order_id" property="orderId" />
	    <result column="cause" property="cause" />
	    <result column="status" property="status" />
	    <result column="create_time" property="createTime" />
	    <result column="process_user_id" property="processUserId" />
	    <result column="process_user_name" property="processUserName" />
	    <result column="process_time" property="processTime" />
	    <result column="result" property="result" />
  	</resultMap>

	<insert id="insert" parameterType="cn.dibcbks.entity.Unqualified" useGeneratedKeys="true" keyProperty="id">
		INSERT INTO  unqualified
	    <trim prefix="(" suffix=")" suffixOverrides=",">
	      <if test="id != null">
	        id,
	      </if>
	      <if test="unitId != null">
	        unit_id,
	      </if>
	      <if test="unitName != null">
	        unit_name,
	      </if>
	      <if test="type != null">
	        type,
	      </if>
	      <if test="orderId != null">
	        order_id,
	      </if>
	      <if test="cause != null">
	        cause,
	      </if>
	      <if test="status != null">
	        status,
	      </if>
	      <if test="createTime != null">
	        create_time,
	      </if>
	      <if test="processUserId != null">
	        process_user_id,
	      </if>
	      <if test="processTime != null">
	        process_time,
	      </if>
	      <if test="result != null">
	        result,
	      </if>	      
	    </trim>
	    <trim prefix="values (" suffix=")" suffixOverrides=",">
	      <if test="id != null">
	        #{id},
	      </if>
	      <if test="unitId != null">
	        #{unitId},
	      </if>
	      <if test="unitName != null">
	        #{unitName},
	      </if>
	      <if test="type != null">
	        #{type},
	      </if>
	      <if test="orderId != null">
	        #{orderId},
	      </if>
	      <if test="cause != null">
	        #{cause},
	      </if>
	      <if test="status != null">
	        #{status},
	      </if>
	      <if test="createTime != null">
	        #{createTime},
	      </if>
	      <if test="processUserId != null">
	        #{processUserId},
	      </if>
	      <if test="processTime != null">
	        #{processTime},
	      </if>
	      <if test="result != null">
	        #{result},
	      </if>	      
	    </trim>
	</insert>
	
	<!--查询用户 -->
	<select id="select" resultMap="BaseResultMap"> 
		SELECT
			q.id,
			q.unit_id,
			q.unit_name,
			q.type,
			q.order_id,
			q.cause,
			q.`status`,
			q.create_time,
			q.process_user_id,
			q.process_time,
			q.result
		FROM
			unqualified AS q
		<if test="where != null">where ${where}</if>
		<if test="orderBy != null">order by ${orderBy}</if>
		<if test="offset != null">limit ${offset},${countPerPage}</if>
	</select>
	
	<!-- 查询单个用户信息 -->
	<select id="selectUnqualifiedById" resultMap="BaseResultMap" parameterType="java.lang.Integer">
		SELECT
			q.id,
			q.unit_id,
			q.unit_name,
			q.type,
			q.order_id,
			q.cause,
			q.`status`,
			q.create_time,
			q.process_user_id,
			u.username AS process_user_name,
			q.process_time,
			q.result
		FROM
			unqualified AS q
		LEFT JOIN user AS u ON u.id = q.process_user_id
		WHERE	
			q.id = #{id}
	</select>
	 

	<update id="updateById"  parameterType="cn.dibcbks.entity.Unqualified">
		update unqualified
	    <set>
	      <if test="id != null">
	        id = #{id},
	      </if>
	      <if test="unitId != null">
	        unit_id = #{unitId},
	      </if>
	      <if test="type != null">
	        type = #{type},
	      </if>
	      <if test="orderId != null">
	        order_id = #{orderId},
	      </if>
	      <if test="cause != null">
	        cause = #{cause},
	      </if>
	      <if test="status != null">
	        status = #{status},
	      </if>
	      <if test="createTime != null">
	        create_time = #{createTime},
	      </if>
	      <if test="processUserId != null">
	        process_user_id = #{processUserId},
	      </if>
	      <if test="processTime != null">
	        process_time = #{processTime},
	      </if>
	      <if test="result != null">
	        result = #{result},
	      </if>	      
	    </set>
	    where id = #{id}
	</update>

	
	<delete id="deleteById" parameterType="java.lang.Integer">
		DELETE FROM unqualified where id = #{id};
	</delete>
</mapper>
	
	
	
	
	
	
	
	
	